{
    "name": "orion-commons",
    "version": "0.1.9",
    "description": "Commons Orion library",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hivesolutions/orion-commons-js.git"
    },
    "license": "Apache-2.0",
    "scripts": {
        "build": "tsc -p .",
        "lint": "eslint \"./**/*.{js,ts,cjs}\"",
        "lint-fix": "eslint \"./**/*.{js,ts,cjs}\" --fix",
        "prettier": "prettier \"./**/*.{js,ts,cjs,json}\" --write",
        "pretty": "npm run prettier && npm run lint-fix",
        "test": "npm run test:mocha",
        "test:mocha": "mocha --require ts-node/register --recursive test",
        "watch": "tsc -p . --watch"
    },
    "type": "module",
    "source": "index.ts",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "typings": "index.ts",
    "peerDependencies": {
        "dotenv": "^16.4.5",
        "fastify": "^5.1.0",
        "hive-js-util": "^0.5.3",
        "yonius": "^0.13.11"
    },
    "devDependencies": {
        "@types/chai": "^4.3.20",
        "@types/mocha": "^10.0.10",
        "@types/node": "^22.9.3",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "chai": "^5.1.2",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.1",
        "fastify": "^5.1.0",
        "hive-js-util": "^0.5.3",
        "mocha": "^10.8.2",
        "mocha-cli": "^1.0.1",
        "prettier": "^3.3.3",
        "react": "^18.2.0",
        "react-dom": "^18.3.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2",
        "yonius": "^0.13.11"
    }
}
